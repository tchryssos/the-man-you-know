    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "ramda": "^0.27.1"
  }
}
